{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "none",
	"jsxSingleQuote": false,
	"bracketSpacing": true,
	"insertPragma": false,
	"vueIndentScriptAndStyle": false,
	"endOfLine": "auto",
  "htmlWhitespaceSensitivity": "ignore",
  "arrowParens": "avoid"
}